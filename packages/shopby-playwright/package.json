{
  "name": "shopby-playwright",
  "version": "1.0.4",
  "description": "ShopBy의 Playwright 기반 테스트 라이브러리",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bomeeyoon-nanuya/playwright.git"
  },
  "homepage": "https://playwright.dev",
  "engines": {
    "node": ">=18"
  },
  "main": "index.js",
  "bin": {
    "shopby-playwright": "./cli.js"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "require": "./index.js",
      "default": "./index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "index.js",
    "index.mjs",
    "index.d.ts",
    "cli.js",
    "custom-codegen.js",
    "scripts/**",
    "ui/**"
  ],
  "scripts": {
    "prepare": "node ./scripts/copy-ui.js",
    "postinstall": "node ./scripts/patch-playwright-core.js"
  },
  "author": {
    "name": "ShopBy"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "playwright-core": "git+https://github.com/bomeeyoon-nanuya/playwright.git#shopby-playwright"
  }
} 